t = ['a', 'b', 'c', 'd', 'e', 'f']

print(t[1:3])  # Output: ['b', 'c']
print(t[:4])   # Output: ['a', 'b', 'c', 'd']
print(t[3:])   # Output: ['d', 'e', 'f']
print(t[:])    # Output: ['a', 'b', 'c', 'd', 'e', 'f']

cloned_list_slicing = t[:]
print(cloned_list_slicing)
cloned_list_list = list(t)
print(cloned_list_list)
cloned_list_copy = t.copy()
print(cloned_list_copy)
